local function DMNAYGZ_fake_script() -- HitBoxbutton.Hitbox Script 
	local script = Instance.new('LocalScript', HitBoxbutton)

	-- Script per il TextButton
	local button = script.Parent
	
	local function onButtonClick()
		print("Hello, here is the Galaxy HitBox modifier")
		print("OP follow us on Youtube and discord")
	
		function getplrsname()
			for i,v in pairs(game:GetChildren()) do
				if v.ClassName == "Players" then
					return v.Name
				end
			end
		end
	
		local players = getplrsname()
		local plr = game[players].LocalPlayer
	
		coroutine.resume(coroutine.create(function()
			while wait(1) do
				coroutine.resume(coroutine.create(function()
					for _,v in pairs(game[players]:GetPlayers()) do
						if v.Name ~= plr.Name and v.Character then
							v.Character.RightUpperLeg.CanCollide = false
							v.Character.RightUpperLeg.Transparency = 30
							v.Character.RightUpperLeg.Size = Vector3.new(13,13,13)
	
							v.Character.LeftUpperLeg.CanCollide = false
							v.Character.LeftUpperLeg.Transparency = 30
							v.Character.LeftUpperLeg.Size = Vector3.new(13,13,13)
	
							v.Character.HeadHB.CanCollide = false
							v.Character.HeadHB.Transparency = 30
							v.Character.HeadHB.Size = Vector3.new(13,13,13)
	
							v.Character.HumanoidRootPart.CanCollide = false
							v.Character.HumanoidRootPart.Transparency = 30
							v.Character.HumanoidRootPart.Size = Vector3.new(13,13,13)
						end
					end
				end))
			end
		end))
	end
	
	button.MouseButton1Click:Connect(onButtonClick)
end
coroutine.wrap(DMNAYGZ_fake_script)()
local function INXUE_fake_script() -- InfiniteAmmoButton.infinite ammo script 
	local script = Instance.new('LocalScript', InfiniteAmmoButton)

	-- LocalScript inside the TextButton
	
	local button = script.Parent
	
	button.MouseButton1Click:Connect(function()
		while wait() do
			local player = game:GetService("Players").LocalPlayer
			local ammoCount = player:WaitForChild("PlayerGui"):WaitForChild("GUI"):WaitForChild("Client"):WaitForChild("Variables"):FindFirstChild("ammocount")
			local ammoCount2 = player:WaitForChild("PlayerGui"):WaitForChild("GUI"):WaitForChild("Client"):WaitForChild("Variables"):FindFirstChild("ammocount2")
	
			if ammoCount then
				ammoCount.Value = 999
			end
	
			if ammoCount2 then
				ammoCount2.Value = 999
			end
		end
	end)
	
end
coroutine.wrap(INXUE_fake_script)()
local function ENPAYY_fake_script() -- EspEnableButton.LocalScript 
	local script = Instance.new('LocalScript', EspEnableButton)

	-- LocalScript inside the TextButton
	
	local button = script.Parent
	
	button.MouseButton1Click:Connect(function()
		-- Your loadstring script
		local success, err = pcall(function()
			loadstring(game:HttpGet('https://raw.githubusercontent.com/Lucasfin000/SpaceHub/main/UESP'))()
		end)
	
		if not success then
			warn("Error executing script: " .. err)
		end
	end)
	
end
coroutine.wrap(ENPAYY_fake_script)()
local function AQKMVL_fake_script() -- Closed.LocalScript 
	local script = Instance.new('LocalScript', Closed)

	-- LocalScript inside the TextButton
	
	local button = script.Parent
	local frame = button.Parent
	local minimized = false
	local UserInputService = game:GetService("UserInputService")
	
	-- Function to toggle the frame's visibility
	local function toggleFrame()
		if not minimized then
			frame.Visible = false -- Hide the frame
			minimized = true
		else
			frame.Visible = true -- Show the frame
			minimized = false
		end
	end
	
	-- Connect button click to toggleFrame function
	button.MouseButton1Click:Connect(toggleFrame)
	
	-- Function to handle key press
	local function onKeyPress(input, gameProcessed)
		if not gameProcessed then
			if input.KeyCode == Enum.KeyCode.LeftAlt or input.KeyCode == Enum.KeyCode.RightAlt then
				toggleFrame()
			end
		end
	end
	
	-- Connect key press to onKeyPress function
	UserInputService.InputBegan:Connect(onKeyPress)
	
end
coroutine.wrap(AQKMVL_fake_script)()
local function UMOMT_fake_script() -- Frame.drag script 
	local script = Instance.new('LocalScript', Frame)

	local UserInputService = game:GetService("UserInputService")
	local TweenService = game:GetService("TweenService")
	
	local frame = script.Parent
	local dragging = true
	local dragInput, mousePos, framePos
	
	local function update(input)
		local delta = input.Position - mousePos
		local newPosition = UDim2.new(
			framePos.X.Scale,
			framePos.X.Offset + delta.X,
			framePos.Y.Scale,
			framePos.Y.Offset + delta.Y
		)
		frame.Position = newPosition
	end
	
	frame.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			mousePos = input.Position
			framePos = frame.Position
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	frame.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if dragging and input == dragInput then
			update(input)
		end
	end)
	
end
coroutine.wrap(UMOMT_fake_script)()
