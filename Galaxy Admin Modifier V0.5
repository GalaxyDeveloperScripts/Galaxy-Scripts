local function MMFW_fake_script() -- Reversebutton.Reverse script 
	local script = Instance.new('LocalScript', Reversebutton)

	local Players = game:GetService("Players")
	local UserInputService = game:GetService("UserInputService")
	local LocalPlayer = Players.LocalPlayer
	
	local reverseButton = script.Parent
	local textBox = script.Parent.Parent:FindFirstChild("Nome Player reverse")
	
	local reversed = true
	
	local Players = game:GetService("Players")
	local UserInputService = game:GetService("UserInputService")
	local LocalPlayer = Players.LocalPlayer
	
	local reverseButton = script.Parent
	local textBox = script.Parent.Parent:FindFirstChild("Nome Player reverse")
	
	local reversed = false
	
	-- Funzione per invertire i movimenti
	local function onInputBegan(input, gameProcessed)
		if gameProcessed then
			return
		end
	
		local character = LocalPlayer.Character
		if not character then return end
		local humanoid = character:FindFirstChildOfClass("Humanoid")
		if not humanoid then return end
	
		if input.UserInputType == Enum.UserInputType.Keyboard then
			local moveDirection = Vector3.new(0, 0, 0)
	
			if input.KeyCode == Enum.KeyCode.W then
				moveDirection = reversed and Vector3.new(0, 0, 1) or Vector3.new(0, 0, -1)
			elseif input.KeyCode == Enum.KeyCode.S then
				moveDirection = reversed and Vector3.new(0, 0, -1) or Vector3.new(0, 0, 1)
			elseif input.KeyCode == Enum.KeyCode.A then
				moveDirection = reversed and Vector3.new(1, 0, 0) or Vector3.new(-1, 0, 0)
			elseif input.KeyCode == Enum.KeyCode.D then
				moveDirection = reversed and Vector3.new(-1, 0, 0) or Vector3.new(1, 0, 0)
			end
	
			humanoid:Move(moveDirection, true)
		end
	end
	
	-- Funzione per attivare/disattivare l'inversione
	local function toggleReverse()
		reversed = not reversed
	end
	
	reverseButton.MouseButton1Click:Connect(function()
		local playerName = textBox.Text
		if playerName == LocalPlayer.Name then
			toggleReverse()
		else
			warn("You can only reverse your own controls in LocalScript.")
		end
	end)
	
	-- Collegamento dell'evento InputBegan per catturare i comandi di movimento
	UserInputService.InputBegan:Connect(onInputBegan)
	
end
